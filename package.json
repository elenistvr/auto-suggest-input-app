    "ts-jest": "^29.1.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
